<launch>
  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
  	<rosparam file="$(find box_finder)/param/move_base_params.yaml" command="load"/>
    <rosparam file="$(find box_finder)/param/costmap_common_params.yaml" command="load" ns="local_costmap"/>
    <rosparam file="$(find box_finder)/param/costmap_common_params.yaml" command="load" ns="global_costmap"/>
    <rosparam file="$(find box_finder)/param/local_costmap_params.yaml" command="load"/>
    <rosparam file="$(find box_finder)/param/global_costmap_params.yaml" command="load"/>
    <rosparam file="$(find box_finder)/param/base_local_planner_params.yaml" command="load"/>
    <rosparam file="$(find box_finder)/param/dwa_local_planner_params.yaml" command="load"/>
    <rosparam file="$(find box_finder)/param/base_global_planner_params.yaml" command="load"/>
    <rosparam file="$(find box_finder)/param/navfn_global_planner_params.yaml" command="load"/>
  </node>
</launch>
